  "main": "index.js",
  "bin": "./index.js",
  "type": "module",
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^2.4.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "semi": false,
    "experimentalTernaries": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "none",
    "singleAttributePerLine": false,
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": false,
    "proseWrap": "preserve",
    "insertPragma": false,
    "printWidth": 80,
    "requirePragma": false,
    "tabWidth": 2,
    "useTabs": false,
    "embeddedLanguageFormatting": "auto"
  },
  "scripts": {
    "start": "pm2 start prod.pm2.json",
    "start:dev": "nodemon --ignore ./esdata/ --ignore ./storage/ --ignore ./react/ --ignore ./node_modules/ index.js",
    "dev": "pm2 start dev.pm2.json",
    "build:react": "cd react && vite build",
    "database": "docker compose up -d",
    "api": "nodemon index.js"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.14.0",
